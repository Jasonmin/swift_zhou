
platform :ios, '8.0'

def all_Pods
use_frameworks!

   pod 'CTMediator', '~> 25'
   pod 'SKPhotoBrowser', '~> 6.1.0'
   pod 'Alamofire', '~> 4.8.2'
#    pod 'CocoaHTTPServer', '~> 2.3'
   pod 'MBProgressHUD', '~> 1.1.0'
   pod 'Charts', '~> 3.3.0'
   
      
end

targetsArray = ['swift_zhou']

targetsArray.each do |t|
    target t do
        all_Pods
    end
end
